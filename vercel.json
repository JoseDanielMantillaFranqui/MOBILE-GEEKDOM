{
    "version": 2,
    "builds": [
      {
        "src": "api/productos.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/productos", "dest": "api/productos.js" },
      { "src": "/(.*)", "dest": "index.html" }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "/" }
    ]
  }
  